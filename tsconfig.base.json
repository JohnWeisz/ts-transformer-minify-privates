{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src",
		"esModuleInterop": true,
		"lib": ["es2015"],
		"newLine": "LF",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"strict": true,
		"stripInternal": true,
		"target": "es5"
	}
}
